<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.djf.app.androiddevelop">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>

    <application
        android:allowBackup="true"

        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SpalshActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".others.SpinnerAndAutoComplete"></activity>
        <activity android:name=".others.SearchActivity"></activity>
        <activity android:name=".others.AnalogClockTest"></activity>
        <activity android:name=".others.DateAndTimeDisplay"
           ></activity>
        <activity android:name=".others.LinkifyTest"></activity>
        <activity android:name=".activityLifecycle.ActivityLifeCycleTest"></activity>
        <activity android:name=".activityLifecycle.NormalActivity"></activity>
        <activity
            android:name=".activityLifecycle.DialogActivity"
            android:theme="@android:style/Theme.Dialog"></activity>
        <activity android:name=".SlidingToFinish.SlidingActivity"
            android:theme="@android:style/Theme.Translucent" ></activity>
        <activity android:name=".SlidingToFinish.SecondActivity"
            android:theme="@android:style/Theme.Translucent"></activity>
    </application>

</manifest>