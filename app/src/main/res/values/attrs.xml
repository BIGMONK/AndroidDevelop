<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 声明一个属性集合，集合的名称一般和拥有这些属性的类同名 -->
    <declare-styleable name="MyCustomView">
        <attr name="customView" format="string" />
        <attr name="customViewColor" format="color" />
        <attr name="customViewSize" format="dimension" />
        <attr name="customViewBackgroundIcon" format="reference" />
    </declare-styleable>

</resources>