<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.djf.app.androiddevelop"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.djf.app.androiddevelop.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.djf.app.androiddevelop.ControllerTest" />
        <activity android:name="com.djf.app.androiddevelop.SearchActivity" />
        <activity android:name="com.djf.app.androiddevelop.AnalogClockTest" />
        <activity android:name="com.djf.app.androiddevelop.DateAndTimeDisplay" />
        <activity android:name="com.djf.app.androiddevelop.LinkifyTest" />
        <activity android:name="com.djf.app.androiddevelop.ActivityLifeCycleTest" />
        <activity android:name="com.djf.app.androiddevelop.NormalActivity" />
        <activity
            android:name="com.djf.app.androiddevelop.DialogActivity"
            android:theme="@android:style/Theme.Dialog" />
    </application>

</manifest>